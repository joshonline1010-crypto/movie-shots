  "camera3d": {
    "_description": "Exact 3D camera placement matching Qwen Tool UI",
    "azimuth": {
      "range": [0, 359],
      "description": "Horizontal rotation around subject (degrees)",
      "presets": {
        "0": { "label": "front", "description": "Front view" },
        "45": { "label": "front-right", "description": "Three-quarter view from right" },
        "66": { "label": "front-right-view", "description": "Front-right view" },
        "90": { "label": "right", "description": "Right profile" },
        "135": { "label": "back-right", "description": "Three-quarter back-right" },
        "180": { "label": "back", "description": "Back view" },
        "225": { "label": "back-left", "description": "Three-quarter back-left" },
        "270": { "label": "left", "description": "Left profile" },
        "315": { "label": "front-left", "description": "Three-quarter view from left" }
      }
    },
    "elevation": {
      "range": [-30, 60],
      "description": "Vertical angle (degrees). -30=low angle, 0=eye level, 60=high angle",
      "presets": {
        "-30": { "label": "low-angle", "description": "Low angle, looking up, heroic" },
        "-15": { "label": "slight-low", "description": "Slightly low, subtle heroic" },
        "0": { "label": "eye-level", "description": "Eye level, neutral" },
        "15": { "label": "slight-elevated", "description": "Slightly elevated" },
        "29": { "label": "elevated-shot", "description": "Elevated shot" },
        "30": { "label": "elevated", "description": "Elevated, looking down" },
        "45": { "label": "high-angle", "description": "High angle, vulnerable" },
        "60": { "label": "birds-eye", "description": "Bird's eye view" }
      }
    },
    "distance": {
      "range": [0.6, 1.8],
      "description": "Camera distance multiplier. 0.6=close-up, 1.0=medium, 1.8=wide",
      "presets": {
        "0.6": { "label": "extreme-close", "description": "Extreme close-up, face detail" },
        "0.7": { "label": "close-up", "description": "Close-up, head and shoulders" },
        "0.8": { "label": "medium-close", "description": "Medium close-up, chest up" },
        "1.0": { "label": "medium", "description": "Medium shot, waist up" },
        "1.2": { "label": "medium-long", "description": "Medium-long, knees up" },
        "1.4": { "label": "wide", "description": "Wide shot, full body" },
        "1.6": { "label": "long", "description": "Long shot, full body with space" },
        "1.8": { "label": "extreme-wide", "description": "Extreme wide, tiny figure" }
      }
    },
    "promptFormat": "Example output: 'three-quarter view from the right, slightly elevated angle, wide full body shot'"
  },

